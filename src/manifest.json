{
  "manifest_version": 2,
  "name": "Github Wiki TOC Generator",
  "version": "1.0",
  "permissions": ["contextMenus"],
  "background": {
    "scripts": ["backgroundscript.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*/wiki*"],
      "js": ["jquery-2.1.1.js", "contentscript.js"]
    }
  ],
  "icons": {
    "16": "table_new.png"
  }
}